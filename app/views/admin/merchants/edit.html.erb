<%= render partial: 'layouts/admin_header'%>

<center><h2 style="background-color:#E0E0E0; border-top: 2px solid black">Edit <%= @merchant.name %></center></h2><br>

<%= form_with url: "/admin/merchants/#{@merchant.id}", method: :patch, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, value: @merchant.name %>
  <%= f.submit "Submit" %>
<% end %>
